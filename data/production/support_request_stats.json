{
    "$schema": "https://raw.githubusercontent.com/moj-analytical-services/etl_manager/master/etl_manager/specs/table_schema.json",
    "name": "support_request_stats",
    "description": "Data table for Operations Engineering support requests",
    "data_format": "csv",
    "columns": [
        {
            "name": "Type",
            "type": "character",
            "description": "Request type"
        },
        {
            "name": "Action",
            "type": "character",
            "description": "Request action"
        },
        {
            "name": "Date",
            "type": "date",
            "description": "Request date"
        }
    ],
    "location": "production",
    "glue_specific": {
        "Parameters": {
            "skip.header.line.count": "1"
        },
        "StorageDescriptor": {
            "Parameters": {
                "skip.header.line.count": "1"
            }
        }
    }
}